{
  "name": "web-template",
  "version": "0.0.1",
  "description": "empty simple web page template",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack --mode development",
    "build-only": "webpack --mode production",
    "build": "del-cli dist && webpack --mode production && html dist/*.html --indent-size 2",
    "watch": "webpack --mode development --watch",
    "start": "webpack-dev-server --mode development --open",
    "test:prod": "del-cli dist && npm run build && static dist -p 8888",
    "beautify": "html dist/*.html --indent-size 2",
    "clear": "del-cli dist"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "del-cli": "^3.0.0",
    "html-cli": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "node-static": "^0.7.11",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^4.0.0",
    "sass-loader": "^8.0.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": ">=3.10.2"
  }
}
