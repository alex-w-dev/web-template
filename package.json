{
  "name": "web-template",
  "version": "0.0.1",
  "description": "empty simple web page template",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack --mode development",
    "build-only": "webpack --mode production",
    "build": "del-cli dist && webpack --mode production && html dist/*.html --indent-size 2",
    "watch": "webpack --mode development --watch",
    "start": "webpack-dev-server --mode development --open",
    "test:prod": "del-cli dist && npm run build && static dist -p 8888",
    "beautify": "html dist/*.html --indent-size 2",
    "clear": "del-cli dist"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@types/pixi.js": "^4.8.3",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.7",
    "del-cli": "^1.1.0",
    "html-cli": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "node-static": "^0.7.11",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": ">=3.1.11"
  }
}
